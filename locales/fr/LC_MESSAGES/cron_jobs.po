msgid "delete_items.pl --where \"items.withdrawn ! 0\" --where \"items.withdrawn_on < $(date --date=\"13 month ago\" --rfc-3339=date)\" --commit"
msgstr "delete_items.pl --where \"items.withdrawn ! 0\" --where \"items.withdrawn_on < $(date --date=\"13 month ago\" --rfc-3339=date)\" --commit"

#: ../../source/cron_jobs.rst:667
msgid "delete_items.pl --where \"itemlost >= '1'\" --where \"itemlost <='4'\" --where \"itemlost_on < '2014-04-28'\" --commit"
msgstr "delete_items.pl --where \"itemlost >= '1'\" --where \"itemlost <='4'\" --where \"itemlost_on < '2014-04-28'\" --commit"

