msgid "Examples:"
msgstr ""

#: ../../source/11_reports.rst:218
#, python-format
msgid ""
"SELECT surname,firstname FROM borrowers WHERE branchcode=<<Enter patrons "
"library\\|branches>> AND surname like <<Enter filter for patron surname "
"(% if none)>>"
