msgid "<<items.content>>"
msgstr ""

#: ../../source/16_cron_jobs.rst:803
msgid ""
"one line for each item, each line containing a tab separated list of "
"title, author, barcode, issuedate"
msgstr ""

#: ../../source/16_cron_jobs.rst:806
msgid "<<borrowers.\\*>>"
msgstr ""

#: ../../source/16_cron_jobs.rst:808
